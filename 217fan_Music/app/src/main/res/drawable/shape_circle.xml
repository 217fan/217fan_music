<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!--solid表示远的填充线-->
    <solid android:color="@color/colorblue"/>
    <!--strock表示远的边框线-->
    <stroke
        android:width="1dp"
        android:color="@color/colorblue"/>
    <!--size控制宽高-->
    <size
        android:width="15dp"
        android:height="15dp"/>
</shape>
